<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_stock_warehouse_orderpoint_tree_editable_inherit_itesm_stock" model="ir.ui.view">
        <field name="name">view.stock.warehouse.orderpoint.tree.editable.inherit.itesm.stock</field>
        <field name="model">stock.warehouse.orderpoint</field>
        <field name="inherit_id" ref="stock.view_warehouse_orderpoint_tree_editable"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='route_id']" position="before">
                <field name="location_warehouse_id" invisible="1"/>
            </xpath>
            <xpath expr="//field[@name='route_id']" position="attributes">
                <attribute name="domain">[('supplied_wh_id.id','=',location_warehouse_id)]</attribute>
            </xpath>
        </field>
    </record>
</odoo>
